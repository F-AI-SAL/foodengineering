# Production env. Copy to .env.production and fill with real values.
PORT=4000
DATABASE_URL=postgresql://username:password@db-host:5432/food_engineering
CORS_ORIGIN=https://admin.example.com,https://app.example.com
WS_PATH=/ws
JWT_SECRET=replace-with-strong-secret
JWT_EXPIRES_IN=1d
APP_URL=https://admin.example.com

