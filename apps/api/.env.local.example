# Local development env. Copy to .env for local use.
PORT=4000
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/food_engineering
CORS_ORIGIN=http://localhost:3000,http://127.0.0.1:3000
WS_PATH=/ws
JWT_SECRET=local-dev-secret-change-me
JWT_EXPIRES_IN=1d
APP_URL=http://localhost:3000

# Uploads
STORAGE_PROVIDER=s3
UPLOAD_MAX_MB=10
UPLOAD_ALLOWED_MIME=image/jpeg,image/png,image/svg+xml,application/pdf
VIRUS_SCAN_ENABLED=false
VIRUS_SCAN_CMD=clamscan
S3_BUCKET=
S3_REGION=
S3_ACCESS_KEY_ID=
S3_SECRET_ACCESS_KEY=
S3_PUBLIC_BASE_URL=
CLOUDINARY_CLOUD_NAME=
CLOUDINARY_API_KEY=
CLOUDINARY_API_SECRET=
CLOUDINARY_FOLDER=food-engineering

# Notifications
EMAIL_PROVIDER=none
EMAIL_FROM=no-reply@foodengineering.com
NOTIFICATION_TEST_EMAIL=ops@foodengineering.com
SMTP_HOST=
SMTP_PORT=587
SMTP_USER=
SMTP_PASS=
SENDGRID_API_KEY=
WHATSAPP_PROVIDER=none
WHATSAPP_FROM=
TWILIO_ACCOUNT_SID=
TWILIO_AUTH_TOKEN=
